import java.awt.*;
import javax.swing.*;

public class GameScreen {
    public static void main(String[] args) {
        JFrame f= new JFrame();
        f.setLocation(0,0);
        f.setSize(400, 400);
        f.setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
        
       TicTacToe board = new TicTacToe();
       f.add(board);
       f.pack();
       f.setVisible(true);
        
    }
}
